<div class='box box-theme box-form form-horizontal'>
<div class='box-header with-border'>_{DETAIL}_ ureports_session_details</div>
<div class='box-body'>
  <div class='form-group'>
    <label class='col-md-3'>#</label>
    <div class='col-md-9'>
      %ID%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{LOGIN}_:</label>
    <div class='col-md-9'>
      %LOGIN%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{SENDED}_</label>
    <div class='col-md-9'>
      %SENDED%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{DESTINATION}_:</label>
    <div class='col-md-9'>
      %DESTINATION%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{TARIF_PLAN}_</label>
    <div class='col-md-9'>
      %TP_NAME%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{REPORTS}_ #</label>
    <div class='col-md-9'>
      %REPORT_ID%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{STATUS}_:</label>
    <div class='col-md-9'>
      %STATUS%
    </div>
  </div>
  <div class='form-group'>
    <label class='col-md-3'>_{MESSAGE}_:</label>
    <div class='col-md-9'>
      %BODY%
    </div>
  </div>

</div>
</div>


