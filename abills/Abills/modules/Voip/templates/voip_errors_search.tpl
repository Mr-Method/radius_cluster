<hr>
<div class='box-form form-horizontal'>
    <div class='form-group'>
        <label class='col-md-3 control-label'>_{MESSAGE}_</label>
        <div class='col-md-6'>
            <input class='form-control' type=text name=MESSAGE value='%MESSAGE%'>
        </div>
    </div>
    <div class='form-group'>
        <label class='col-md-3 control-label'>_{ACTION}_</label>
        <div class='col-md-6'>
            %ACTIONS_SEL%
        </div>
    </div>
    <div class='form-group'>
        <label class='col-md-3 control-label'>_{TYPE}_</label>
        <div class='col-md-6'>
            %LOG_TYPE_SEL%
        </div>
    </div>
    <div class='form-group'>
        <label class='col-md-3 control-label'>_{NAS}_</label>
        <div class='col-md-6'>
            %NAS_ID_SEL%
        </div>
    </div>
</div>


