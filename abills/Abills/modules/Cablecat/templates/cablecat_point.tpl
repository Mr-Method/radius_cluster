<hr/>

<div class='form-group'>
  <label class='control-label col-md-3' for='CREATED_ID'>_{CREATED}_</label>
  <div class='col-md-9'>
    <input type='text' class='form-control datepicker' value='%CREATED%'  name='CREATED'  id='CREATED_ID'  />
  </div>
</div>

<div class='checkbox text-center'>
  <label>
    <input type='checkbox' data-return='1' data-checked='%PLANNED%' name='PLANNED'   id='PLANNED_ID'  />
    <strong>_{PLANNED}_</strong>
  </label>
</div>

<div class='form-group'>
  <label class='control-label col-md-3' for='COMMENTS_ID'>_{COMMENTS}_</label>
  <div class='col-md-9'>
    <textarea class='form-control col-md-9'  rows='5'  name='COMMENTS' id='COMMENTS_ID'>%COMMENTS%</textarea>
  </div>
</div>